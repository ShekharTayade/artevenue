{% load static %}
{% load utils %}
{% load indian_numbers_tags %}


<div id = "sizes_paper_unframed" style="display:none;">
	<div class = "row justify-content-center" style = "padding: 10px;">
		{% for key, value in ready_prod_data_paper.items %}
			<div style = "padding: 0px 5px 10px;">
			<p class ="text-center" style = "margin:0px; cursor:pointer;"
				onclick = "set_ready_product('PAPER', 'PAPER_ROLLED', {{ value.PAPER_WIDTH_ROLLED}}, {{value.PAPER_WIDTH_ROLLED}}, {{value.PAPER_WIDTH_UNFRAMED}}, {{value.PAPER_HEIGHT_UNFRAMED}}, {{value.MOULDING_ID}}, {{value.MOUNT_ID}}, {{value.MOUNT_SIZE}}, '{{value.MOUNT_COLOR}}', {{value.ACRYLIC_ID}},  {{value.BOARD_ID}}, {{value.STRETCH_ID}}, true);">
				<span id= "paper-size-ROLLED-{{value.PAPER_WIDTH_ROLLED|floatformat}}"  style = "margin:0px; border:1px solid #ddd; cursor:pointer; padding:7px;">
					{{ value.PAPER_WIDTH_ROLLED|floatformat }}" X {{ value.PAPER_HEIGHT_ROLLED|floatformat}}"
				</span>
			</p>
			<h4 style = "margin-top:5px;" class = "text-center">										
				<i id = "price-{{key}}-paper-unframed" class = "fa fa-inr"> {{ value.PAPER_PRICE_ROLLED|floatformat|intcomma_indian }} </i>
			</h4>
			</div>
		{% endfor %}
	</div>
</div>


